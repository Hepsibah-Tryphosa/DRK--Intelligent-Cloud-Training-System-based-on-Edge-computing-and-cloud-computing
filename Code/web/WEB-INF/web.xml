<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Dbconn</servlet-name>
        <servlet-class>DatabaseConnectivity.Dbconn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>owner_login</servlet-name>
        <servlet-class>LoginAction.owner_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>owner_register</servlet-name>
        <servlet-class>LoginAction.owner_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fileUpload</servlet-name>
        <servlet-class>FileUpload.fileUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_login</servlet-name>
        <servlet-class>LoginAction.user_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_register</servlet-name>
        <servlet-class>LoginAction.user_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fileUploadaction</servlet-name>
        <servlet-class>FileUpload.fileUploadaction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dbconn</servlet-name>
        <url-pattern>/Dbconn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>owner_login</servlet-name>
        <url-pattern>/owner_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>owner_register</servlet-name>
        <url-pattern>/owner_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fileUpload</servlet-name>
        <url-pattern>/fileUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_login</servlet-name>
        <url-pattern>/user_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_register</servlet-name>
        <url-pattern>/user_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fileUploadaction</servlet-name>
        <url-pattern>/fileUploadaction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
